# Ethereum RPC URL (optional - uses MetaMask's provider by default)
VITE_ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY

# Solana RPC URL (optional - uses public endpoint by default)
VITE_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Network Configuration
VITE_ETHEREUM_CHAIN_ID=1
VITE_SOLANA_NETWORK=mainnet-beta

# Feature Flags
VITE_ENABLE_TESTNET=false
VITE_ENABLE_ANALYTICS=false
